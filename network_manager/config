    config DEFAULT_IP4_ADDRESS
        string "AP Channel to use"
        default "192.168.1.2"

    config DEFAULT_IP4_NETMASK
        string "Default IPv4 netmask"
        default "255.255.255.0"

    config DEFAULT_IP4_GATEWAY
        string "Default IPv4 gateway"
        default "192.168.1.1"

    config DEFAULT_IP4_DNS
        string "Default IPv4 DNS"
        default "1.1.1.1"

    config DEFAULT_IP4_AP_GATEWAY
        string "Default IPv4 AP gateway"
        default "192.168.1.1"

    config DEFAULT_SNTP
        string "Default SNTP"
        default "pool.ntp.org"

endmenu